   "source": [
    "from sklearn.externals.joblib import Parallel, delayed\n",
    "import time\n",
    "\n",
    "def slow_square(i):\n",
    "    time.sleep(5)\n",
    "    return i*i\n",
    "\n",
    "t = time.time()\n",
    "r = Parallel(n_jobs=3)(delayed(slow_square)(i) for i in range(3))\n",
    "print r, time.time() - t"
   ]
